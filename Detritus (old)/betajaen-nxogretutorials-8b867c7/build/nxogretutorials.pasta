        - %OGRE_HOME%/include/OIS/        
        - %OGRE_HOME%/boost_1_42/
        - %NXOGRE_DIR%/../critter/sdk/
      libraries:
        - %OGRE_HOME%/lib/debug/OgreMain_d.lib if debug
        - %OGRE_HOME%/lib/release/OgreMain.lib ifn debug
        - %OGRE_HOME%/lib/debug/OIS_d.lib if debug
