        - %OGRE_HOME%/boost_1_42/lib/
        - %NXOGRE_DIR%/../critter/sdk/CritterDebugStatic.lib if debug
        - %NXOGRE_DIR%/../critter/sdk/CritterStatic.lib ifn debug
      sources:
        - Sandbox
      after:
        - ../VC.postbuild.bat ^NAME^ ^CONFIGURATION^ debug if debug
        - ../VC.postbuild.bat ^NAME^ ^CONFIGURATION^ release ifn debug
